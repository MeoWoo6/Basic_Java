package hello;

import java.util.Scanner;

public class Week501 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] poly = new int[2][10];
		int power;
		int coef;
		Scanner in = new Scanner(System.in);
		power = in.nextInt();
		coef = in.nextInt();
		int max1 = power;
		boolean first = true;
		boolean second = false;
		while(first)
		{
			poly[0][power] = coef;
			if(power==0)
			{
				first = false;
				second = true;
			}	
		}
		
		power = in.nextInt();
		coef = in.nextInt();
		int max2 = power;
		
		while(second)
		{
			poly[1][power] = coef;
			if(power==0)
			{
				second = false;
				break;
			}
			power = in.nextInt();
			coef = in.nextInt();
			
		}
		int max;
		if(max1>max2)
		{
			max = max1;
		}
		else
		{
			max = max2;
		}
		while(!first && !second)
		{
//			int sum_coef = poly[0][max] + poly[1][max];
//			if(sum_coef!=0)
//			{
//				System.out.print(sum_coef+"x"+max);
//			}
//			
//			for(int i=max; i>=0; i--)
//			{
//				sum_coef = poly[0][i]+poly[1][i];
//				
//			}
		}
		

	}

}
