package hello;

import java.util.Scanner;

public class Week402 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x;
		int count=0;
		boolean neg = false;
		Scanner in = new Scanner(System.in);
		x = in.nextInt();
		if (x<0)
		{
			count += 1;
			neg = true;
		}
		do
		{
			x /= 10;
			count += 1;
		} while(x>0);
		
		

	}

}
