package hello;

public class Week701 {
	
	public static void Fac(int a)
	{
		for(int i=2; i<a; i++)
		{
			if(a % i ==0)
			{
				System.out.print(i+"x");
				Fac(a/i);
				break;
			}
		}
		System.out.print(a);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Integer factorization
		

	}

}
